        {name = "brown", emoji = "üü´", color = Color3.fromRGB(139, 69, 19), row = 2},
        {name = "cyan", emoji = "üü¶", color = Color3.fromRGB(0, 255, 255), row = 2},
        {name = "pink", emoji = "üå∏", color = Color3.fromRGB(255, 105, 180), row = 2},
        {name = "lime", emoji = "üü¢", color = Color3.fromRGB(50, 205, 50), row = 2},
        {name = "gray", emoji = "‚ö™", color = Color3.fromRGB(128, 128, 128), row = 2},
        {name = "maroon", emoji = "üî¥", color = Color3.fromRGB(128, 0, 0), row = 2},
