    local clear = Instance.new("TextButton")
    clear.Name = "clear"
    clear.Parent = Frame
    clear.BackgroundColor3 = Color3.fromRGB(180, 80, 80)
    clear.BorderColor3 = Color3.fromRGB(0, 0, 0)
    clear.BorderSizePixel = 0
    clear.Position = UDim2.new(0.05, 0, 0.880470445, 0)
