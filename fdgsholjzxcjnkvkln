 local Button = ToolsTab:CreateButton({
    Name = "Flashlight (Larrys Night Guard)",
    Callback = function()
        
--Flashlight -- SCRIPT NOT MADE BY ME
function sandbox(var,func)
	local env = getfenv(func)
	local newenv = setmetatable({},{
		__index = function(self,k)
			if k=="script" then
				return var
			else
				return env[k]
			end
		end,
	})
	setfenv(func,newenv)
	return func
end
cors = {}
